{
  "name": "eis-ui-render",
  "version": "0.31.5",
  "description": "UI generation based on JSON schemas",
  "homepage": "https://eisgroup.github.io/ui-render/",
  "license": "Apache 2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "static/",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "craco start --verbose",
    "build": "craco build && cp build/index.html build/404.html",
    "build-lib": "webpack --mode production --config webpack.library.config.js && npm run gen-ts",
    "watch-lib": "webpack --watch --mode production --config webpack.watch.config.js",
    "yalc-publish": "npm run build-lib && yalc publish --push",
    "yalc-watch": "nodemon --watch src --ext js,jsx,ts,tsx --exec 'npm run yalc-publish'",
    "gen-ts": "tsc --project tsconfig.build.json",
    "serve-dist": "serve dist -p 3102",
    "serve-build": "serve build -p 3103",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.11.13",
    "deep-equal": "^2.2.3",
    "flat": "^4.1.0",
    "humanize-duration": "^3.32.1",
    "lodash-es": "^4.17.21",
    "moment": "2.29.4",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "rc-input-number": "^8.5.0",
    "rc-picker": "4.11.3",
    "rc-slider": "^9.3.1",
    "react-animate-height": "^2.0.21",
    "react-json-tree": "^0.11.2",
    "react-lines-ellipsis": "^0.14.1",
    "react-markdown": "^8.0.7",
    "react-rating": "^2.0.5",
    "react-sizeme": "^2.6.12",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^1.8.5",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-toc": "^9.0.0",
