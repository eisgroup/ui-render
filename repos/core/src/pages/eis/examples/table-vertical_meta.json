{
  "view": "Table",
  "name": "coverages",
  "vertical": true,
  "headers": [
    {
      "id": "coverageID",
      "label": "Coverage Name",
      "renderCell": {
        "view": "Expand",
        "name": "{value}",
        "index": "{index}",
        "onClick": "handleItemExpand"
      }
    },
    {
      "id": "fundingStructure.contributionType",
      "label": "Contribution Type"
    },
    {
      "id": "fundingStructure.requiredParticipationPrct",
      "label": "Contribution",
      "renderCell": "Percent"
    },
    {
      "id": "rate",
      "label": "Quote Rate",
      "renderCell": "double5"
    },
    {
      "id": "annualPremium",
      "label": "Annual Premium",
      "renderCell": "Currency"
    },
    {
      "id": "#",
      "renderHeader": {
        "view": "Checkbox",
        "label": "Expand All",
        "onChange": "handleToggleExpandAll"
      }
    }
  ]
}
